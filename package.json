{
  "name": "web2k17",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "githooks": "sh build/git-hooks/install.sh",
    "test": "node config/testing.js --env=jsdom",
    "deploy:ghpages": "git-directory-deploy --directory dist --branch gh-pages"
  },
  "dependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
